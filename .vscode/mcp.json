{
	"servers": {
		"convex-mcp": {
			"type": "stdio",
			"command": "npx",
			"args": ["-y", "convex@latest", "mcp", "start"]
		},
		"shadcn": {
			"command": "npx",
			"args": ["shadcn@latest", "mcp"]
		},
		"tanstack": {
			"url": "https://tanstack.com/api/mcp"
		},
		"workos": {
			"command": "npx",
			"args": ["-y", "@workos/mcp-docs-server"]
		}
	}
}
